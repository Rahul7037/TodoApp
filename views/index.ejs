<header>
  <%- include("./partials/nav.ejs") %>
</header>
<div class="blogs content">
  <h2>Todos List</h2>

  <% if (users.length> 0) { %>
    <% users.forEach(eachuser=> { %>
      <a class="single" href="/users/<%= eachuser.id %>">
        <h3 class="title">
          <%= eachuser.name %>
        </h3>
        <p class="snippet">
          <%= eachuser.email%>
        </p>
      </a>
      <% }) %>
        <% } else { %>
          <p>There are no users to display...</p>
          <% } %>

</div>